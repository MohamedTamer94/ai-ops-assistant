# Backend runtime
ENV=docker

# Secrets
JWT_SECRET=change-this-in-prod
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# AI provider
GROQ_API_KEY=replace-me
GROQ_LLM_MODEL=llama-3.3-70b-versatile

# CORS (JSON arrays expected by pydantic-settings)
ALLOWED_ORIGINS=["http://localhost:5173"]
CORS_ALLOW_METHODS=["GET","POST","PUT","DELETE"]

# Optional compose overrides
POSTGRES_USER=aiops
POSTGRES_PASSWORD=aiops
POSTGRES_DB=aiops
